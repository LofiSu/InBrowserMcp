{
  "name": "in-browser-mcp",
  "version": "0.1.0",
  "description": "浏览器自动化工具套件，包含服务器和浏览器扩展",
  "private": true,
  "scripts": {
    "build": "pnpm run build:server",
    "build:server": "cd server && pnpm run build",
    "start:server": "cd server && pnpm start",
    "dev:server": "cd server && pnpm run dev",
    "direct:server": "cd server && node dist/index.js",
    "dev-frontend": "cd frontend && pnpm run dev"
  },
  "workspaces": [
    "server",
    "frontend"
  ]
}
